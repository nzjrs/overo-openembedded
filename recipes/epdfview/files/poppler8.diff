+void
+DocumentPage::invertRegion (GdkRegion* region)
+{
+    int count;
+    GdkRectangle *rectangles;
+    gdk_region_get_rectangles(region, &rectangles, &count);
+    while(count--){
+        GdkRectangle r = rectangles[count];
+        invertArea(r.x, r.y, r.x + r.width, r.y + r.height);
+    }
+    g_free(rectangles);
+}
